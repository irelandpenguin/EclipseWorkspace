	public static void main(String[] args) {
		byte[] bys = {105,68,124,75,111,118,91,101,121,87,105,100,89,97};
		String s = "";
		for(int i = 0; i < bys.length; i++) {
			byte b = bys[i];
			if (b >= 97) {
				char c = (char)b;//强制把字节转成字符
				s += c;//拼接起来
			}
		}
		System.out.println(s);
	}
}

