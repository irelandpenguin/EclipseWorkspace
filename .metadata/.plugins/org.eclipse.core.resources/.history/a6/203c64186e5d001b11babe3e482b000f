package com.irelandpenguin_06;

class String01 {
	public static void main(String[] args) {
		byte[] bys = { 105, 68, 124, 75, 111, 118, 91, 101, 121, 87, 105, 100,
				89, 97 };
		String s = new String();
		for (int i = 0; i < bys.length; i++) {
			int num = bys[i];
			if (num >= 97) {
//				char c = (char) num;
//				s += c;
				s += (char)num;
			}

		}
		System.out.println(s);
	}
}