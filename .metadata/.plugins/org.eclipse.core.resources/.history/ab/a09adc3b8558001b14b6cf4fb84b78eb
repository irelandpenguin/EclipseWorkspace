package com.irelandpenguin_06;
/**
 * 父类
 * @author irelandpenguin
 *
 */
public abstract class Father {
	//成员变量
	private String name;
	//抽象方法
	public abstract void study();//学习
	public abstract void play();//玩
	
	//成员方法
	public void setName(String Name){
		this.name=name;
	}
	public String getName(){
		return this.name;
	}
}
